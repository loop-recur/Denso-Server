<div class="content-box">
  <div class="content-box-header">
    <h3>Edit App</h3>
    <%= tabs ["Cancel", admin_apps_path] %>
  </div>
  <div class="content-box-content">
    <%= form_for([:admin, @app]) do |f| %>
      <%= f.error_messages %>
      <%= render :partial => "form", :object => f %>
      <div class="submit"><%= f.submit %></div>
    <% end %>
  </div>
</div>
